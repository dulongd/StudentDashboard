@model IEnumerable<StudentDashboard.Models.Course>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Courses</h2>

@if (!Model.Any())
{
    <p>No courses have been added yet.</p>
}
else
{
    <ul>
        @foreach (var course in Model)
        {
            <li>@course.CourseName</li>
        }
    </ul>
}